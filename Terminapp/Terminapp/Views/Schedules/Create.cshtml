@{
ViewData["Title"] = "Opprett Terminliste";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="Create">
    <div class="form-group">
        <label for="StartDate">Startdato</label>
        <input type="date" name="startDate" required class="form-control" />
    </div>
    <div class="form-group">
        <label for="EndDate">Sluttdato</label>
        <input type="date" name="endDate" required class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Generer Terminliste</button>
</form>
